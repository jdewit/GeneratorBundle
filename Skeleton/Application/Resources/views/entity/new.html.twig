{{ "{% extends '"~ bundle_name ~"::layout.html.twig' %}" }}
{{ "{% block content %}" }}
    <h1 class="headline">New {{ entity }}</h1>
    <div class="content">  
        <form method="post" action="{{ "{{ path('"~ bundle_alias ~"_"~ entity_lc ~"_new') }}" }}" {{ "{{ form_enctype(form) }}" }}>  
            {{ "{% include '"~ bundle_name ~":"~ entity ~":form.html.twig' %}" }}
            <div>
                <input type="submit" class="button" title="Create a new {{ entity_lc }}" value="Create {{ entity }}"/>
            </div>
        </form>
    </div>            
{{ "{% endblock %}" }}

