<?xml version="1.0" encoding="UTF-8" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="{{ bundle_alias }}.{{ entity_us }}_manager'.class">{{ bundle_vendor }}\{{ bundle_basename }}\Entity\Manager\{{ entity }}Manager</parameter>
    </parameters>

    <services>
        <service id="{{ bundle_alias }}.{{ entity_us }}_manager'.default" class="%{{ bundle_alias }}.{{ entity_us }}_manager'.class%" public="false">
            <argument type="service" id="{{ bundle_alias }}.entity_manager" />
            <argument>%{{ bundle_alias }}.model.{{ entity_cc }}.class%</argument>
        </service>
    </services>

</container>