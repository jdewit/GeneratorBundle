{
    "name": "{{ bundleVendor | lower }}/{{ bundleCoreName | lower }}-bundle",
    "type": "symfony-bundle",
    "description": "Symfony2 {{ bundleCoreName }} Bundle",
    "keywords": ["{{ bundleCoreName | lower }}"],
    "homepage": "http://github.com/{{ bundleVendor | lower }}/{{ bundleBaseName }}",
    "license": "MIT",
    "authors": [
        {
            "name": "Joris de Wit",
            "email": "joris.w.dewit@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.2"
    },
    "autoload": {
        "psr-0": { "{{ bundleVendor }}\\{{ bundleBaseName }}": "" }
    },
    "target-dir": "{{ bundleVendor }}/{{ bundleBaseName }}"
}
